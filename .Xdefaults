XTerm*vt100.modifyOtherKeys: 1

URxvt.font: xft:Monospace:size=12:antialias=true
URxvt.scrollBar: false
URxvt.title: Terminal
URxvt.perl-lib: /usr/lib/urxvt/perl/
URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select,matcher,tabbed
URxvt.keysym.M-u: perl:url-select:select next
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-c: perl:clipboard:paste_escaped
URxvt.url-select.launcher: chromium
URxvt.url-select.underline: true
URxvt.matcher.button: 1
URxvt.clipboard.copycmd:  xsel -ib
URxvt.clipboard.pastecmd: xsel -ob

*fading: 40
*fadeColor: #002b36
*foreground: #839496
*background: #002b36
*color0: #073642
*color1: #dc322f
*color2: #859900
*color3: #b58900
*color4: #268bd2
*color5: #d33682
*color6: #2aa198
*color7: #eee8d5
*color8: #002b36
*color9: #cb4b16
*color10: #586e75
*color11: #657b83
*color12: #cb4b16
*color13: #6c71c4
*color14: #93a1a1
*color15: #fdf6e3
